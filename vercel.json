{
	"version": 2,
	"outputDirectory":"dist",
	"builds": [
		{
				"src": "./index.js",
				"use": "@vercel/node"
		}
	],
	"routes": [
				{ "src": "/(.*)", "dest": "/" },
				{
					"src": "/api/(.*)",
					"dest": "server.js"
			},
			{
					"src": "/",
					"dest": "public/index.html"
			},
			{
					"src": "/(.+)",
					"dest": "public/$1"
			}
	],
	"static": {
		"public": "public",
		"dist": "dist" 
	}
}