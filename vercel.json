{
    "version": 2,
    "outputDirectory":"dist",
    "builds": [
      {
         "src":"dist/**",
         "use":"@vercel/vite"
      },
      {
         "src": "./index.js",
         "use": "@vercel/node" 
    	}
    ],
    "routes": [
         { "src": "/(.*)", "dest": "/" },
         {
            "src": "/(.+)",
            "dest": "dist/$1"
         }
    ],
    "framework":"vite"
 }